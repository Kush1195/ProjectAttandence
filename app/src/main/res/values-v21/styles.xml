<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:colorPrimaryDark">@color/colorWhite</item>
    </style>
    <style name="table_style">
        <item name="android:background">@drawable/s_bg_table</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="table_style_end">
        <item name="android:background">@drawable/s_bg_table_end</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="header_style">
        <item name="android:background">@drawable/s_header_table</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="header_style_end">
        <item name="android:background">@drawable/s_header_table_end</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="text_header">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/text_color_b</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>

    </style>

    <style name="text_table">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color_small</item>
        <item name="android:singleLine">true</item>

    </style>

    <style name="text_time_table">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color_b</item>
        <item name="android:singleLine">true</item>

    </style>

    <style name="text_sub">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/text_color_b</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="header_style_time">
        <item name="android:background">@drawable/s_header_table</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color_b</item>
        <item name="android:singleLine">true</item>

    </style>
</resources>